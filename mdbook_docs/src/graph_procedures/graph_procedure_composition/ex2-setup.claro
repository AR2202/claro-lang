newtype A: int
newtype FooRes : int

function doFoo(a: A, barRes: BarRes) -> future<FooRes> {
  _ = a; _ = barRes;
  return futures::immediateFuture(FooRes(-1));
}
function doFooA(c: C) -> future<A> {
  _ = c;
  return futures::immediateFuture(A(-1));
}
function doFooC(a: A) -> future<C> {
  _ = a;
  return futures::immediateFuture(C(-1));
}